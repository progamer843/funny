function Slam()
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
		local SlamAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Slam)
		--local PushAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Grab)
		--PushAnim:Play(0.1, 0.9, 1)
		SlamAnim:Play(0.1, 1, 2.5)
		SlamAnim:AdjustSpeed(15)
		--PushAnim:AdjustSpeed(1))
		PlayAudio(130944130, 100, 1)
		wait(0.02)
		HitBox(Player.Character["Right Arm"], 6.125, 0.225, 0.27, 99, Vector3.new(0, -500, 0), 35, "rbxassetid://4164190231", true)
		--PushAnim:Stop(0.3)
		Move = false
	end)
end
function StandSlam()
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
StandAppearence(true)
		local SlamAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.StandSlam)
		--local PushAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Grab)
		--PushAnim:Play(0.1, 0.9, 1)
		SlamAnim:Play(0.1, 1, 2.5)
		SlamAnim:AdjustSpeed(2.5)
		--PushAnim:AdjustSpeed(1))
		PlayAudio(1582127763, 100, 1)
		wait(0.02)
		HitBox(Player.Character.Stand["Stand Right Arm"], 6.125, 0.225, 0.27, 50, Vector3.new(0, -500, 0), 35, "rbxassetid://4164190231", true)
		wait(0.8)
	StandAppearence(false)
		--PushAnim:Stop(0.3)
		Move = false
	end)
end
Mouse.KeyDown:Connect(function(Key)
    		if Key == "f" then
    		if Bruh == false then
			Slam()
    		elseif Bruh == true then
		    StandSlam()
		    end
    		end
end)
function BeatdownHumanoid(Humanoid)
	Beatdown = true
	Move = true
	pcall(function()
	    StandAppearence(true)
		local BarrageAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Barrage)
		BarrageAnim:Play(0.1, 1, 2)
		BarrageAnim:AdjustSpeed(2.25)
		PlayAudio(5004316774, 6, 1.069)
		pcall(function()
			if Player.Character.Stand:FindFirstChild("StandRarm") then
				rStorage.Trail:FireServer(Player.Character.Stand.StandRarm.Trail, true)
				rStorage.Trail:FireServer(Player.Character.Stand.StandLarm.Trail, true)
			else
				rStorage.Trail:FireServer(Player.Character.Stand["Stand Right Arm"].Trail, true)
				rStorage.Trail:FireServer(Player.Character.Stand["Stand Left Arm"].Trail, true)
			end
		end)
		rStorage.SuperArmor:FireServer(true)
		rStorage.Deflect:FireServer(true)
		rStorage.Epitaph:FireServer(true)
		--rStorage.VampireRush:FireServer(Humanoid, CFrame.new(0, -30, 0), -30000, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
		rStorage.BurnDamage:FireServer(Humanoid, Humanoid.Parent.Head.CFrame, -900000, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
		coroutine.resume(coroutine.create(function()
			repeat
				--rStorage.VampireRush:FireServer(Humanoid, CFrame.new(0, -30, 0), -300, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
				rStorage.VampireDamage:FireServer(Humanoid, 1e-200, 0.25, Vector3.new(0, 0, -0))
				rStorage.Knock:FireServer(Humanoid)
				wait(0.95)
			until Beatdown == false
		end))
		coroutine.resume(coroutine.create(function()
			repeat
				Player.Character.Humanoid.WalkSpeed = 0
				Player.Character.Humanoid.JumpPower = 0
				Damage:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame * CFrame.new(math.random(-15,15)/100, math.random(-15,15)/100, math.random(-15,15)/100), 45, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0.195, Color3.fromRGB(132, 182, 141), "rbxassetid://1693499499", math.random(95,115)/100, 1)
				wait(0.1)
			until Beatdown == false
		end))
		wait(20)
		ABDSound.StopAudio(5004316774)
		Beatdown = false
		wait(0.2)
		pcall(function()
			if Player.Character.Stand:FindFirstChild("StandRarm") then
				rStorage.Trail:FireServer(Player.Character.Stand.StandRarm.Trail, false)
				rStorage.Trail:FireServer(Player.Character.Stand.StandLarm.Trail, false)
			else
				rStorage.Trail:FireServer(Player.Character.Stand["Stand Right Arm"].Trail, false)
				rStorage.Trail:FireServer(Player.Character.Stand["Stand Left Arm"].Trail, false)
			end
		end)
		BarrageAnim:Stop(0.075)
		StandAppearence(false)
		local PunchAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Punch)
		PunchAnim:Play(0.1, 1, 2)
		PunchAnim:AdjustSpeed(1.35)
		PlayAudio(5002748505, 6, 1.06)
		wait(0.2)
		Player.Character.Humanoid.WalkSpeed = 16
		Player.Character.Humanoid.JumpPower = 50
		rStorage.SuperArmor:FireServer(false)
		rStorage.Deflect:FireServer(false)
		rStorage.Epitaph:FireServer(false)
		rStorage.Dodge:FireServer()
		Damage:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame * CFrame.new(math.random(-15,15)/100, math.random(-15,15)/100, math.random(-15,15)/100), 69, 0.25, Humanoid.Parent.Torso.CFrame:Inverse().lookVector * 65, "rbxassetid://241837157", 0.25, Color3.fromRGB(255, 0, 0), "rbxassetid://441202925", math.random(95,115)/100, 1.5)
		wait(3.25)
		PlayAudio(142684400, 10, 1)
		wait(2.15)
		PlayAudio(5128453901, 15, 1, false, 0, Humanoid.Parent)
		wait(1.85)
		rStorage.Damage12:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame, 1e-200, 0, Vector3.new(0,0,0), 0.5, "rbxassetid://", 1, 1)
		rStorage.Damage3:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame, 1e-200, 0, Vector3.new(0,0,0), 1, "rbxassetid://", 1, 1)
		for i = 1,200,1 do
		rStorage.ULFDamage:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame, 1e-200, 0, Vector3.new(0,0,0), 50, "rbxassetid://", 1, 1)
		end
		rStorage.Damage12Sans:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame, 1e-200, 0, Vector3.new(0,0,0), 6, "rbxassetid://", 1, 1)
		rStorage.Damage11Enderman:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame, 1e-200, 0, Vector3.new(0,0,0), 1, "rbxassetid://", 1, 1)
		rStorage.BurnDamage:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame, 9e999, 0.3, Humanoid.Parent.Torso.CFrame:Inverse().lookVector * 1e44 * Vector3.new(nil, nil, nil), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://2050481695", 1, 7.5)
	    rStorage.GravityForce:FireServer()
		--rStorage.VampireRush:FireServer(Humanoid, CFrame.new(0, -30, 0), 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
		coroutine.resume(coroutine.create(function()
			for i = 1, 10 do
				rStorage.Damage12:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0.3, Vector3.new(-1,-1,-1) * 9e999, 1e-320, "rbxassetid://1693499499", 1, 0)
				rStorage.ULFDamage:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 1e-320, "rbxassetid://1693499499", 1, 0)
				rStorage.Damage12Sans:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 1e-320, "rbxassetid://1693499499", 1, 0)
				rStorage.Damage11Enderman:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 1e-320, "rbxassetid://1693499499", 1, 0)
				rStorage.Damage3:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 1e-320, "rbxassetid://1693499499", 1, 0)
			end
		end))
	end)
	Beatdown = false
	Move = false
end
function BeatdownGrab()
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
		local GrabAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Grab)
		GrabAnim:Play(0.1, 1, 2)
		GrabAnim:AdjustSpeed(1.05)
		wait(0.25)
		HitBox(Player.Character["Left Arm"], 6, 0.25, 0.3, 1, Vector3.new(0, 0, 0), 0, "", true, false, 0, false, false, true)
		wait(0.4)
		GrabAnim:Stop(0.25)
		if Beatdown == true then
			repeat wait() until Beatdown == false
		end
		Move = false
	end)
end
function MOVELOL(Character)
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
		if Player.Character:FindFirstChild("Animate") then
			Player.Character.Animate.Disabled = true
		end
		local PlayingAnims = {}
		local function Check(Anim)
			for Index, Animations in next, PlayingAnims do
				if PlayingAnims[Index][1] == Anim then
					return PlayingAnims[Index][2]
				end
			end
			return 1
		end
		for Index, Animation in next, Player.Character.Humanoid:GetPlayingAnimationTracks() do
			table.insert(PlayingAnims, {Animation, Animation.Speed})
			Animation:AdjustSpeed(0)
		end
		if Character:FindFirstChildOfClass("Humanoid").Health > 0.01 then
			Move = true
			rStorage.SuperArmor:FireServer(false)
			rStorage.Deflect:FireServer(false)
			rStorage.Epitaph:FireServer(false)
			rStorage.VampireDash:FireServer(Player.Character.Torso.BrickColor)
                        Player.Character:SetPrimaryPartCFrame(Character.Torso.CFrame * CFrame.new(0, 0.5, 3.65))
                        		local PunchAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.UpperCut)
		PlayAudio(5002748505, 7, 1.05)
		PunchAnim:Play(0.1, 1, 2.5)
		PunchAnim:AdjustSpeed(3)
			wait(0.06)
			if FirstTime == true then
				Damage:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame, 0, 0.5, Vector3.new(0,100,0), "rbxassetid://241837157", 4.65, Character.Torso.Color, "rbxassetid://1058693352", 1, 1)
				wait(1)
				Damage:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame, 0, 35, Vector3.new(0,0,0), "rbxassetid://241837157", 4.65, Character.Torso.Color, "rbxassetid://1058693352", 1, 1)
				FirstTime = false
			else
				Damage:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame, 0, 0.5, Vector3.new(0,100,0), "rbxassetid://241837157", 4.65, Character.Torso.Color, "rbxassetid://", 1, 1)
				wait(1)
			Damage:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame, 0, 35, Vector3.new(0,0,0), "rbxassetid://241837157", 4.65, Character.Torso.Color, "rbxassetid://", 1, 1)
			end
			Move = true
			rStorage.SuperArmor:FireServer(true)
			rStorage.Deflect:FireServer(true)
			rStorage.Epitaph:FireServer(true)
			rStorage.VampireDash:FireServer(Player.Character.Torso.BrickColor)
			wait(1)
			Player.Character:SetPrimaryPartCFrame(Character.Torso.CFrame * CFrame.new(0, 0.5, 3.65))
			wait(0.03)
			
			game.workspace.Gravity = 0
			Player.Character.Humanoid.WalkSpeed = 0
			Player.Character.Humanoid.JumpPower = 0
			Gay = true
			PlayAudio(5004316774, 6884581, 1.069)
			rStorage.BurnDamage:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame, -9999, 0.3, Character.Torso.CFrame:Inverse().lookVector * 0 * Vector3.new(nil, nil, nil), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://2050481695", 1, 7.5)
		coroutine.resume(coroutine.create(function()
			local RollAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.BeatDownBarrage)
			repeat
			    game:GetService("ReplicatedStorage").VampireDash:FireServer(BrickColor.new("Transparent"))
				RollAnim:Play(0.1, 1, 2.5)
				RollAnim:AdjustSpeed(5)
				Player.Character:SetPrimaryPartCFrame(Character.Torso.CFrame * CFrame.new(0, 0.5, 3.65))
									local Hit = math.random(1, 4)
			if Hit == 1 then
			    rStorage.Knock:FireServer(Character:FindFirstChildOfClass("Humanoid"))
				rStorage.Damage11:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(math.random(-25,25)/10, math.random(-15,15)/10, math.random(-15,15)/10) * CFrame.Angles(math.rad((math.random(-15,15)/13)*9), math.rad((math.random(-15,15)/13)*9), math.rad((math.random(-15,15)/13)*9)), 0, 5, Vector3.new(0, 0, 0), 0.75, "rbxassetid://1693499499", 1, 0)
			elseif Hit == 2 then
			    rStorage.Knock:FireServer(Character:FindFirstChildOfClass("Humanoid"))
				rStorage.Damage11Sans:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(math.random(-25,25)/10, math.random(-15,15)/10, math.random(-15,15)/10) * CFrame.Angles(math.rad((math.random(-15,15)/13)*9), math.rad((math.random(-15,15)/13)*9), math.rad((math.random(-15,15)/13)*9)), 0, 5, Vector3.new(0, 0, 0), 0.75, "rbxassetid://1693499499", 1, 0)
			elseif Hit == 3 then
			    rStorage.Knock:FireServer(Character:FindFirstChildOfClass("Humanoid"))
				rStorage.Damage11Enderman:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(math.random(-25,25)/10, math.random(-15,15)/10, math.random(-15,15)/10) * CFrame.Angles(math.rad((math.random(-15,15)/13)*9), math.rad((math.random(-15,15)/13)*9), math.rad((math.random(-15,15)/13)*9)), 0, 5, Vector3.new(0, 0, 0), 0.75, "rbxassetid://1693499499", 1, 0)
			elseif Hit == 4 then
			    rStorage.Knock:FireServer(Character:FindFirstChildOfClass("Humanoid"))
				rStorage.ULFDamage:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(math.random(-25,25)/10, math.random(-15,15)/10, math.random(-15,15)/10) * CFrame.Angles(math.rad((math.random(-15,15)/13)*9), math.rad((math.random(-15,15)/13)*9), math.rad((math.random(-15,15)/13)*9)), 0, 5, Vector3.new(0, 0, 0), 0.75, "rbxassetid://1693499499", 1, 0)
			end
				wait(0.1)
			until Gay == false
			RollAnim:Stop(0.25)
		end))
		    	
		wait(20)
		Gay = false
		ABDSound.StopAudio(5004316774)
		wait()
			local cringe = Player.Character.Humanoid:LoadAnimation(AnimationTable.Bad)
			cringe:Play(0.1,1,1)
			cringe:AdjustSpeed(1.35)
			PlayAudio(5002748505, 6541156, 1.06)
			wait(.3)
			rStorage.Damage12:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 1, "rbxassetid://1693499499", 1, 0)
			rStorage.Damage12Sans:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 2, "rbxassetid://1693499499", 1, 0)
			rStorage.Damage12Enderman:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 2, "rbxassetid://1693499499", 1, 0)
			wait(2)
			PlayAudio(851889797, 10, 1)
						coroutine.resume(coroutine.create(function()
				wait(2.85)
				Player.Character.Head.Anchored = true
				for i = 1, 6 do
rStorage.Damage12:FireServer(Player.Character:FindFirstChildOfClass("Humanoid"), Player.Character.Head.CFrame * CFrame.new(0, 0.85, -0.1), 0, 0, Vector3.new(0,0,0), 0.025, "rbxassetid://", 1, 1)
					wait(0.035)
				end
			end))
			wait(4.35)
			rStorage.BurnDamage:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame, 9e999, 0.3, Character.Torso.CFrame:Inverse().lookVector * 1e44 * Vector3.new(nil, 50, nil), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://2050481695", 1, 7.5)
			for i = 1,500,1 do
			rStorage.ULFDamage:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 200, "rbxassetid://1693499499", 1, 0)
			end
			for i = 1,500,1 do
			rStorage.Damage11:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 1, "rbxassetid://1693499499", 1, 0)
			rStorage.Damage11Sans:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 2, "rbxassetid://1693499499", 1, 0)
			rStorage.Damage11Enderman:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 2, "rbxassetid://1693499499", 1, 0)
			end
			--rStorage.VampireRush:FireServer(Character:FindFirstChildOfClass("Humanoid"), CFrame.new(0, -30, 0), 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
			coroutine.resume(coroutine.create(function()
				for i = 1, 10 do
					rStorage.Damage12:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0.3, Vector3.new(-1,-1,-1) * 9e999, 1e-320, "rbxassetid://1693499499", 1, 0)
					rStorage.Damage3:FireServer(Character:FindFirstChildOfClass("Humanoid"), Character.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 1e-320, "rbxassetid://1693499499", 1, 0)
				end
			end))
			wait(0.5)
			Player.Character.Humanoid.WalkSpeed = 16
			Player.Character.Humanoid.JumpPower = 50
			wait(0.05)
			Player.Character.Head.Anchored = false
			rStorage.SuperArmor:FireServer(false)
			rStorage.Deflect:FireServer(false)
			rStorage.Dodge:FireServer()
			rStorage.Epitaph:FireServer(false)
			Move = false
			game.Workspace.Gravity = 182
					for Index, Animation in next, Player.Character.Humanoid:GetPlayingAnimationTracks() do
			Animation:AdjustSpeed(Check(Animation))
		end
		if Player.Character:FindFirstChild("Animate") then
			Player.Character.Animate.Disabled = false
		end
		Player.Character.Humanoid.WalkSpeed = 16
		Player.Character.Humanoid.JumpPower = 50
end
end)
end
Mouse.KeyDown:Connect(function(Key)
    if Key == "n" then
        if Bruh == true then
			BeatdownGrab()
        elseif Bruh == false then
	         			if Mouse.Target and Mouse.Target.Parent and Mouse.Target.Parent:FindFirstChildOfClass("Humanoid") then
				MOVELOL(Mouse.Target.Parent)
	         			end
        end 		
    end
    end)
function LiterallySummonGod()
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
		local AllahAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.UIRoar)
		Player.Character.Humanoid.WalkSpeed = 0
		Player.Character.Humanoid.JumpPower = 0
		AllahAnim:Play(0.1, 1, 1)
		PlayAudio(3367974639, 5000, 0.5)
		coroutine.resume(coroutine.create(function()
		repeat
		wait(0.01)
		-- Audio Removed due to it being bypassed, not leaking it.
		HitBox(Player.Character.Head, 26, 0.4, 0.69, 80, Vector3.new(math.random(-6,6)*50*(math.random(1, 4)), 125, math.random(-6,6)*50*(math.random(1, 4))) , 3, nil, false, false, 0, false, false, false, false, nil, true)
game:GetService("ReplicatedStorage").Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame,1,0.1,Vector3.new(0, 0, 0),"rbxassetid://3370684035",50,Color3.new(250, 84, 75),"rbxassetid://1091083826",0,0)
		until Move == false
end))
		wait(4)
		Player.Character.Humanoid.WalkSpeed = 16
		Player.Character.Humanoid.JumpPower = 50
		Move = false
	end)
end

function KaiokenHits(Target)
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
		local HoldAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.UIHand)
		HoldAnim:Play(0.1, 1, 1)
		Player.Character:SetPrimaryPartCFrame(Target.Torso.CFrame * CFrame.new(2, 0.2, -10))
		coroutine.resume(coroutine.create(function() 
			wait(0.28)
			Player.Character.HumanoidRootPart.Anchored = true
		end))
		coroutine.resume(coroutine.create(function()
			repeat wait()
			until HoldAnim.TimePosition > 0.705
			HoldAnim:AdjustSpeed(0)
		end))
		wait(0.025)
		PlaySoundEffect(Player.Character.Humanoid, 915341277, 7.6, 1)
		wait(0.325)
		PlaySoundEffect(Target.Humanoid, 5628558743, 50, 1)
		local Nums = {
			10,
			20,
			30,
			40,
			50,
			60
		}
		for i = 1, 75 do
			wait(0.01)
			rStorage.Damage:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Torso.CFrame * CFrame.new(math.random(-20,20)/10, math.random(-20,20)/10, math.random(-20,20)/10), 10, 0.7, Vector3.new(0, 0, 0), "rbxassetid://5482711860", math.random(10,30)/125, Color3.new(200, 200, 200), "rbxassetid://5482711860", 1, 0)
			rStorage.Damage11Enderman:FireServer(Player.Character.Humanoid, Target.Torso.CFrame, 0, 0, Vector3.new(0,0,0), 0.15, "rbxassetid://2835079178", 1, 0)
			if table.find(Nums, i) then
				--PlayAudio(5628558743, 50, 1, false, 0, Target)
				PlaySoundEffect(Target.Humanoid, 5628558743, 50, 1)
			end
		end
		rStorage.Damage:FireServer(Target:FindFirstChildOfClass("Humanoid"), Target.Torso.CFrame, 75, 0, Vector3.new(0,0,0), "rbxassetid://5482711860", 0.5, Color3.new(175, 175, 175), "rbxassetid://638861091", 1, 10)
		ABDSound.StopAudio(5628558743)
		wait(0.19)
		HoldAnim:Stop(0.1)
		wait(0.07)
		Player.Character.HumanoidRootPart.Anchored = false
		ABDSound.StopAudio(5628558743)
		coroutine.resume(coroutine.create(function()
			wait(0.1)
			for i = 1, 10 do wait(0.1)
				ABDSound.StopAudio(5628558743)
			end
		end))
		Move = false
	end)
end
game:GetService("UserInputService").InputBegan:Connect(function(Input, N)
	if N then
		return
	end
	if Input.KeyCode == Enum.KeyCode.LeftAlt and Move == false then
		LiterallySummonGod()
	elseif Input.KeyCode == Enum.KeyCode.LeftControl and Move == false then
		Kaioken()
	elseif Input.KeyCode == Enum.KeyCode.RightControl and Move == false then
		if Mouse.Target and Mouse.Target.Parent and Mouse.Target.Parent:FindFirstChildOfClass("Humanoid") then
			KaiokenHits(Mouse.Target.Parent)
		end
	end
end)
-- TimeStop Movement:
local RawGameTable = getrawmetatable(game)
local oldNamecall = RawGameTable.__namecall
setreadonly(RawGameTable, false)
 
-- (RTZ) Counter
rStorage.RTZClient.OnClientEvent:connect(function(GivenPlayer)
	if GivenPlayer ~= Player then
		local Humanoid = GivenPlayer.Character.Humanoid
		rStorage.RTZDamage:FireServer(Humanoid, 99)
		Player.Character:SetPrimaryPartCFrame(Humanoid.Parent.Torso.CFrame * CFrame.new(0, 0, 8.5))
		rStorage.KnockAdvanced:FireServer(Humanoid)
		rStorage.Damage:FireServer(Humanoid, Humanoid.Parent.Torso.CFrame, 150, 3, Humanoid.Parent.Torso.CFrame:Inverse().lookVector * 1200, "rbxassetid://5482711860", 0, Color3.new(255, 255, 255), "rbxassetid://2050481695", 1, 7.5)
		Countering = false
		for i = 1, 3 do
			PlayAudio(211540835, 50, 1)
		end
	end
end)

