    if key == 'g' then
        game.ReplicatedStorage:FindFirstChild('EmperorBackdash'):FireServer()
gun()
end
end)
function MadladSpinner()
    if Bruh == false then
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
		if Player.Character:FindFirstChild("Animate") then
			Player.Character.Animate.Disabled = true
		end
		local PlayingAnims = {}
		local function Check(Anim)
			for Index, Animations in next, PlayingAnims do
				if PlayingAnims[Index][1] == Anim then
					return PlayingAnims[Index][2]
				end
			end
			return 1
		end
		for Index, Animation in next, Player.Character.Humanoid:GetPlayingAnimationTracks() do
			table.insert(PlayingAnims, {Animation, Animation.Speed})
			Animation:AdjustSpeed(0)
		end
		local SpinAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Spin)
		PlayAudio(1279559473, 600, 1)
		SpinAnim:Play(0.1, 1, 5)
		SpinAnim:AdjustSpeed(5)
		local Repeater = 0 
		Player.Character.Humanoid.WalkSpeed = 30
		Player.Character.Humanoid.JumpPower = 75
		repeat 
			wait(0.2)
			Player.Character.Humanoid.WalkSpeed = 30
			Player.Character.Humanoid.JumpPower = 75
			Repeater = Repeater + 1
			HitBox(Player.Character.Torso, 6, 0.35, 0.5, 10e-320, Player.Character.Torso.CFrame.lookVector * 5500 + Vector3.new(0, 5000, 0), 0.3)
		until Repeater > 150 or Move == false
		ABDSound.StopAudio(1279559473)
		SpinAnim:Stop(0.2)
		for Index, Animation in next, Player.Character.Humanoid:GetPlayingAnimationTracks() do
			Animation:AdjustSpeed(Check(Animation))
		end
		if Player.Character:FindFirstChild("Animate") then
			Player.Character.Animate.Disabled = false
		end
		Player.Character.Humanoid.WalkSpeed = 16
		Player.Character.Humanoid.JumpPower = 50
		Move = false
	end)
elseif Bruh == true then
    	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
		if Player.Character:FindFirstChild("Animate") then
			Player.Character.Animate.Disabled = true
		end
		local PlayingAnims = {}
		local function Check(Anim)
			for Index, Animations in next, PlayingAnims do
				if PlayingAnims[Index][1] == Anim then
					return PlayingAnims[Index][2]
				end
			end
			return 1
		end
		for Index, Animation in next, Player.Character.Humanoid:GetPlayingAnimationTracks() do
			table.insert(PlayingAnims, {Animation, Animation.Speed})
			Animation:AdjustSpeed(0)
		end
		local StandFly = Player.Character.Humanoid:LoadAnimation(AnimationTable.StandFly)
		PlayAudio(5885680263, 600, 1)
		StandFly:Play(0.1, 1, 1)
		StandFly:AdjustSpeed(1)
		local Repeater = 0 
		StandAppearence(true)
		Player.Character.Humanoid.WalkSpeed = 30
		Player.Character.Humanoid.JumpPower = 75
		repeat 
			wait(0.2)
			Player.Character.Humanoid.WalkSpeed = 30
			Player.Character.Humanoid.JumpPower = 75
			Repeater = Repeater + 1
			HitBox(Player.Character.Torso, 6, 0.35, 0.5, 99, Player.Character.Torso.CFrame.lookVector * 5500 + Vector3.new(0, 5000, 0), 0.3)
					wait(5)
		rStorage.RTZEffect:FireServer(true)
		until Repeater > 150 or Move == false
StandAppearence(false)
		ABDSound.StopAudio(5885680263)
		StandFly:Stop(0.2)
		for Index, Animation in next, Player.Character.Humanoid:GetPlayingAnimationTracks() do
			Animation:AdjustSpeed(Check(Animation))
		end
		if Player.Character:FindFirstChild("Animate") then
			Player.Character.Animate.Disabled = false
		end
		Player.Character.Humanoid.WalkSpeed = 16
		Player.Character.Humanoid.JumpPower = 50
		Move = false
	end)
end
end
mouse.KeyDown:Connect(function(key)
    if key == 'x' then
        MadladSpinner()
    end
end)
function Teleport()
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		rStorage.VampireDash:FireServer(Player.Character.Torso.BrickColor)
		rStorage.FOTPSTeleport:FireServer()
		Player.Character:SetPrimaryPartCFrame(Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -20))
		PlayAudio(2642969814, 10, 1.125)
	end)
end
mouse.KeyDown:Connect(function(key)
    if key == 'v' then
        Teleport()
    end
end)
Mouse.KeyDown:Connect(function(Key)
	if (Move == false and IgnoreAllCooldown == false) then
		if Key == "y" then
			if Invincible == true then
				Starman("Off")
			else
				Move = true
				--PlayAudio(3105789396, 9, 1)
				--wait(1.25)
				Starman("On")
			end
		end
	end
end)
