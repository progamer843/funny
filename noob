function SuperKick()
    if Bruh == false then
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
		local KickAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.Karate)
		PlayAudio(2547662688, 1000, 1)
		KickAnim:Play(0.1, 1, 2.5)
		KickAnim:AdjustSpeed(1.25)
		wait(0.34)
		HitBox(Player.Character["Right Leg"], 4.45, 0.28, 0.3, 99, (Player.Character.HumanoidRootPart.CFrame.lookVector * 9669) + Vector3.new(0, 350, 0), 0.75, "rbxassetid://3240049847", false, true, 10.35)
		wait(0.1)
		Move = false
	end)
elseif Bruh == true then
    	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
	StandAppearence(true)
		local KickAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.StandKick)
		PlayAudio(2547662688, 1000, 1)
		KickAnim:Play(0.1, 1, 2.5)
		KickAnim:AdjustSpeed(1.25)
		wait(0.34)
		HitBox(Player.Character.Stand["Stand Right Leg"], 4.45, 0.28, 0.3, 99, (Player.Character.HumanoidRootPart.CFrame.lookVector * 9669) + Vector3.new(0, 350, 0), 0.75, "rbxassetid://3240049847", false, true, 10.35)
		wait(0.1)
		Move = false
	StandAppearence(false)
end)
end
end
local mouse = game.Players.LocalPlayer:GetMouse()
mouse.KeyDown:Connect(function(key)
    if key == 'k' then
        SuperKick()
    end
end)
_G.strongPunch = function()
    local arm = game.Players.LocalPlayer.Character["Right Arm"]

    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = arm.CFrame
    hitbox1.Name = "hitbox2"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    local weld = Instance.new("WeldConstraint", arm)
    weld.Part0 = hitbox1
    weld.Part1 = arm

    hitbox1.Size = arm.Size * 1.2
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(0,0,0)
    game.Debris:AddItem(hitbox1, .4)

    local debounce =false

    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
            if debounce == false then
                debounce = true
                print(hit.Parent.Name)
                delay(.001, function() debounce = false end)
            game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)
            game.ReplicatedStorage.Damage:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 75, .25, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 100 + Vector3.new(25, 50, 0), "rbxassetid://241837157", .5, Color3.new(151, 0, 0), "rbxassetid://3932506183", 1, 50)
            hitbox1:Destroy()
        end
    elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
        if debounce == false then
            debounce = true
            print(hit.Parent.Name)
            delay(.001, function() debounce = false end)
        game.ReplicatedStorage.Knock:FireServer(hit.Parent.Parent.Humanoid)
        game.ReplicatedStorage.Damage:FireServer(hit.Parent.Parent.Humanoid, hit.Parent.Parent.HumanoidRootPart.CFrame, 75, .25, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 100 + Vector3.new(0, 100, 0), "rbxassetid://241837157", .5, Color3.new(151, 0, 0), "rbxassetid://3932506183", 1, 50)
        hitbox1:Destroy()
        end
    end
    end)
end

mouse.KeyDown:Connect(function(key)
    if key == '' then
        _G.anim3:Play(.1,1,5)
        game.ReplicatedStorage.Damage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0,0,0), "", 0, Color3.new(0,0,0), "rbxassetid://663234710", 1, 2)
        _G.strongPunch()
    end
end)
function MouseTeleport()
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		rStorage.Yell:FireServer()
		wait(0.015)
		rStorage.VampireDash:FireServer(Player.Character.Torso.BrickColor)
		Player.Character:SetPrimaryPartCFrame(CFrame.new(Mouse.Hit.p + Vector3.new(0, 3.5, 0)))
		PlayAudio(289556450, 8, 1)
		wait(0.1)
		rStorage.Yell:FireServer()
	end)
end
_G.omtAttack = function()
    game.ReplicatedStorage.Yell:FireServer()
    game.ReplicatedStorage.GravityForce:FireServer()
    game.ReplicatedStorage.PlayerStrongPunch2:FireServer(Vector3.new(30,5,30), Vector3.new(25,-1,25), BrickColor.new("Transparent"))
     game.ReplicatedStorage.PlayerStrongPunch2:FireServer(Vector3.new(30,5,30), Vector3.new(20,-1,20), BrickColor.new("Transparent"))
      game.ReplicatedStorage.PlayerStrongPunch2:FireServer(Vector3.new(30,5,30), Vector3.new(15,-1,15), BrickColor.new("Transparent"))
       game.ReplicatedStorage.PlayerStrongPunch2:FireServer(Vector3.new(30,5,30), Vector3.new(10,-1,10), BrickColor.new("Transparent"))
        game.ReplicatedStorage.PlayerStrongPunch2:FireServer(Vector3.new(30,5,30), Vector3.new(5,-1,5), BrickColor.new("Transparent"))
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 50 then
                game.ReplicatedStorage.Damage12Enderman:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame , 1,0, Vector3.new(0, 0, 0), 3,"rbxassetid://606138807", 1, 0)
                game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame , 1,0, Vector3.new(0, 0, 0), 3,"rbxassetid://606138807", 1, 0)
                game.ReplicatedStorage.Damage12:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame , 1,0, Vector3.new(0, 0, 0), 3,"rbxassetid://606138807", 1, 0)
                game.ReplicatedStorage.BurnDamage:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame,  math.huge, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://606138807", 1, 10)
                for i = 1,25,1 do
                game:GetService("ReplicatedStorage").HamonSword:FireServer(v.Humanoid,1,1,Vector3.new(9999e999,99e9,9999e9))
                end
                game.ReplicatedStorage.Knock:FireServer(v.Humanoid)
            end
        end
    end
end

mouse.KeyDown:Connect(function(key)
    if key == ',' then
        _G.anim2:Play(.1, 1, 1)
        wait(1)
        PlayAudio(1687284211, 100, 1)
        _G.omtAttack()
    end
end)
_G.lmbMove = function()

for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 50 then
            game.ReplicatedStorage.Damage:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame,  1, 35,game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 1500 + Vector3.new(0, 0, 0) + Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://5855427447", 1, 10)
            game.ReplicatedStorage.Knock:FireServer(v.Humanoid)
            end
        end
    end
    end

mouse.KeyDown:Connect(function(key)
    if key == 'b' then
    _G.lmbMove()
    wait(.3)
    PlayAudio(5855427447, 10, 1)
end
end)
function Uppercut()
    if Bruh == false then
	pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
		local PunchAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.UpperCut)
		PlayAudio(5002748505, 7, 1.05)
		PunchAnim:Play(0.1, 1, 2.5)
		PunchAnim:AdjustSpeed(1.25)
		pcall(function()
			if Player.Character.Stand:FindFirstChild("StandRarm") then
				rStorage.Trail:FireServer(Player.Character.Stand.StandRarm.Trail, true)
			else
				rStorage.Trail:FireServer(Player.Character.Stand["Stand Right Arm"].Trail, true)
			end
		end)
		wait(0.34)
		HitBox(Player.Character["Left Arm"], 4.15, 0.28, 0.3, 99, (Player.Character.HumanoidRootPart.CFrame.lookVector * 6969) + Vector3.new(0, 2250, 0), 0.75, "rbxassetid://3240049847", false, true, 10.35)
		wait(0.1)
		pcall(function()
			if Player.Character.Stand:FindFirstChild("StandRarm") then
				rStorage.Trail:FireServer(Player.Character.Stand.StandRarm.Trail, false)
			else
				rStorage.Trail:FireServer(Player.Character.Stand["Stand Right Arm"].Trail, false)
			end
		end)
		Move = false
	end)
elseif Bruh == true then
    pcall(function()
		if Player.Character.Parent == workspace.Entities then
			if not Player.Character:FindFirstChild("Humanoid") then
				return
			end
		else
			return
		end
		Move = true
		local PunchAnim = Player.Character.Humanoid:LoadAnimation(AnimationTable.StandCutMyDickAss)
		PlayAudio(5688671477, 700, 1.05)
		PunchAnim:Play(0.1, 1, 2.5)
		PunchAnim:AdjustSpeed(3)
		pcall(function()
			if Player.Character.Stand:FindFirstChild("StandRarm") then
				rStorage.Trail:FireServer(Player.Character.Stand.StandRarm.Trail, true)
			else
StandAppearence(true)
				rStorage.Trail:FireServer(Player.Character.Stand["Stand Right Arm"].Trail, true)
			end
		end)
		wait(0.34)
		HitBox(Player.Character["Left Arm"], 4.15, 0.28, 0.3, 99, (Player.Character.HumanoidRootPart.CFrame.lookVector * 6969) + Vector3.new(0, 2250, 0), 0.75, "rbxassetid://3240049847", false, true, 10.35)
		wait(0.1)
		pcall(function()
			if Player.Character.Stand:FindFirstChild("StandRarm") then
				rStorage.Trail:FireServer(Player.Character.Stand.StandRarm.Trail, false)
			else
				rStorage.Trail:FireServer(Player.Character.Stand["Stand Right Arm"].Trail, false)
			end
		end)
StandAppearence(false)
		Move = false
    end)
end
end

mouse.KeyDown:Connect(function(key)
    if key == 'l' then
        Uppercut()
    end
end)
function standgun()
StandAppearance(true)
local playGun = Player.Character.Humanoid:LoadAnimation(AnimationTable.StandPunch)
PlayGun:Play(0.1,1,1)
PlayGun:Adjustspeed(5)
wait()
StandAppearance(false)
game.ReplicatedStorage:FindFirstChild('EmperorBackdash'):FireServer()
end


