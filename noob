	StarmanDamage = not StarmanDamage
	ButtonsFrame.DmgBool.Text = tostring(StarmanDamage)
end)
HubGui.HideButton.MouseButton1Click:Connect(function()
	if HubGui.HideButton.Text == "<" then
		TauntFrame.Visible = false
		SongsFrame.Visible = false
		ButtonsFrame.Visible = false
		HubGui.HideButton.Text = ">"
	else
		ButtonsFrame.Visible = true
		HubGui.HideButton.Text = "<"
	end
end)

