game:GetService("Lighting").TEer:GetPropertyChangedSignal("Value"):Connect(function()
	local Value = game:GetService("Lighting").TEer.Value
	if Value:IsA("Model") and Value.Parent == workspace.Entites and RTZEnabled == false then
		wait(1)
		TimeEraseShow(Value)
	elseif Value:IsA("Player") and RTZEnabled == false then
		wait(1)
		TimeEraseShow(Value.Character)
	end
end)

