-- Functions & Setup:
function CheckPreloaded(ID)
	for Index, Song in next, Preloaded do
		if ID == Song[1] then
			return true
		end
	end
	return nil
end

function CheckObjects(ID, Get, Index)
	for Index, Table in next, Objects do
		if ID == Table[1] then
			if Get == "Index" then
				return Index
			elseif Get == "Value" then
				return Table[Index]
			end
		end
	end
	return nil
end

function GetTimePos(ID)
	for Index, Song in next, Preloaded do
		if ID == Song[1] then
			return Song[2]
		end
	end
	return 20
end

function IsStarmanSong(ID)
	for Index, Table in next, Songs do
		if Table.ID == ID then
			return true
		end
	end
	return nil
end

function PreloadAudio(SoundId)
	local NS = Instance.new("Sound")
	NS.SoundId = "rbxassetid://"..SoundId
	NS.Parent = rStorage
	game:GetService("ContentProvider"):PreloadAsync({NS})
	wait(0.5)
	table.insert(Preloaded, {SoundId, NS.TimeLength})
	wait(0.5)
	NS:Destroy()
end

function StandAppearence(Show)
	if not Player.Character:FindFirstChild("Stand") then return end
	for Index, Child in next, Player.Character.Stand:GetDescendants() do
		if (Child:IsA("BasePart") or Child:IsA("Decal")) and Child.Name ~= "StandHumanoidRootPart" and Child.Name ~= "IntValue" then
			if Show == true then
				rStorage["Transparency"]:FireServer(Child, 0)
			else
				rStorage["Transparency"]:FireServer(Child, 1)
			end
		end
	end
end

function TimeEraseShow(TimeEraser)
	for Index, Child in next, TimeEraser:GetDescendants() do
		if (Child:IsA("BasePart") or Child:IsA("Decal")) and Child.Name ~= "HumanoidRootPart" and Child.Name ~= "StandHumanoidRootPart" then
			Child.Transparency = 0.65
		end
	end
end

function AddButton(Parent, Name, Text, Number)
	local Button = Instance.new("TextButton")
	Button.Size = UDim2.new(0, 108, 0, 30)
	Button.Text = Text
	Button.Name = Name
	Button.TextScaled = true
	Button.Parent = Parent
	local Num = #TauntFrame:GetChildren()
	TauntFrame.CanvasSize = UDim2.new(0, 0, Num/35, 0)
	coroutine.resume(coroutine.create(function()
		Button.MouseButton1Click:Connect(function()
			if Parent == TauntFrame then
				if Posing == true then
					Posing = false
				else
					Taunt(Number)
				end
			elseif Parent == SongsFrame then
				if Invincible == true then
					Starman("Off")
				else
					Starman("On", Number)
				end
			end
		end)
	end))
end

function ABDSound.new(SoundId, Volume, Pitch, Loop, LoopTimes, CustomPlayer)
	if CheckPreloaded(SoundId) == nil then
		PreloadAudio(SoundId)
	end
	local Object = {}
	Object.Length = GetTimePos(SoundId)
	local TP = Player
	if TP == nil then
		return
	end
	local TC = CustomPlayer or TP.Character
	if not TC then
		return
	elseif TC.Parent == nil then
		return
	end
	local hum = TC:FindFirstChildOfClass("Humanoid")
	local rp = TC.HumanoidRootPart
	local trs = TC.Torso
	local Start = tick()
	local Increaser = 0
	local Looped = 0
	local LoopMethod = "Number"
	if LoopTimes == "Infinite" then
		LoopMethod = "Infinite"
	end
	Object.Start = Start
	Object.ID = SoundId
	function Object:Update()
		--Damage:FireServer(hum, rp.CFrame * CFrame.new(0, -0.25, 0), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, trs.Color, "rbxassetid://"..SoundId, Pitch, Volume);
		rStorage.Damage12:FireServer(hum, rp.CFrame, 0, 0, Vector3.new(0, 0, 0), 9e999, "rbxassetid://"..SoundId, Pitch, Volume/4.5);
	end
	function Object:FixTime(Sound)
		game:GetService("RunService").RenderStepped:Wait()
		Sound.TimePosition = (tick() - Start) * Pitch
	end
	Object:Update()
	table.insert(Objects, {SoundId, Object, Increaser, Start})
	ABDSound.Sounds["rbxassetid://"..SoundId] = Object
	local function LoopSong()
		Start = tick()
		Object.Start = Start
		Increaser = 0
		Looped = Looped + 1
		if LoopMethod == "Number" then
			if Looped < LoopTimes then
				coroutine.resume(coroutine.create(function()
					while Increaser < ((Object.Length*3.5) / Pitch) and Object ~= nil do
						if ((tick() - Object.Start) * Pitch) > (Object.Length * Pitch) then
							if Looped >= LoopTimes then
								ABDSound.Sounds[Object] = nil
								ABDSound.StopAudio(Object.ID)
							end
							if Loop == true and Looped < LoopTimes then
								LoopSong()
							end
							break
						else
							Object:Update()
						end
						wait(0.69)
						Increaser = Increaser + 1.15
					end
				end))
			end
		elseif LoopMethod == "Infinite" then
			coroutine.resume(coroutine.create(function()
				while Increaser < ((Object.Length*3.5) / Pitch) and Object ~= nil do
					if ((tick() - Object.Start) * Pitch) > (Object.Length * Pitch) then
						LoopSong()
						break
					else
						Object:Update()
					end
					wait(0.69)
					Increaser = Increaser + 1.15
				end
			end))
		end
	end
	coroutine.resume(coroutine.create(function()
		local NoDo = false
		while Increaser < ((Object.Length*3.5) / Pitch) and Object ~= nil do
			if ((tick() - Object.Start) * Pitch) > (Object.Length * Pitch) then
				if Loop ~= true then
					ABDSound.Sounds[Object] = nil
					ABDSound.StopAudio(Object.ID)
				end
				if Invincible == true and IsStarmanSong(SoundId) ~= nil then
					NoDo = true
					local Song = GetRandomStarmanSong()
					PlayAudio(Song[1], Song[2], Song[3])
				end
				if Loop == true and NoDo == false then
					LoopSong()
				end
				break
			else
				Object:Update()
			end
			wait(0.69)
			Increaser = Increaser + 1.15
		end
	end))
end

function ABDSound.StopAll()
	for i,v in next, ABDSound.Sounds do
		v.Start = 0.1
		v.Length = 0.1
		ABDSound.Sounds[i] = nil
		v = nil
	end
	ABDSound.Sounds = {}
end

function ABDSound.StopAudio(ID)
	for i,v in next, ABDSound.Sounds do
		if v.ID == ID then
			v.Start = 0.1
			v.Length = 0.1
			ABDSound.Sounds[i] = nil
			v = nil
		end
	end
end

function God(Humanoid)
	if Humanoid.Health < 9e9 then
		--rStorage.VampireRush:FireServer(Humanoid, CFrame.new(0, -30, 0), -9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
		rStorage.BurnDamage:FireServer(Humanoid, Humanoid.Parent.Head.CFrame, -math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
		wait(0.5)
		rStorage.BurnDamage:FireServer(Humanoid, Humanoid.Parent.Head.CFrame, math.huge,0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
	end
end

function StarmanHitBox()
	coroutine.resume(coroutine.create(function()
		pcall(function()
			local Humanoids = workspace.Entities:GetChildren()
			local Time = 0
			while true do
				Humanoids = workspace.Entities:GetChildren()
				for i = 1, #Humanoids do
					local Model = Humanoids[i]
					if Model ~= Player.Character then
						if Model:FindFirstChildOfClass("Humanoid") and Model:FindFirstChild("HumanoidRootPart") then
							if (Model.HumanoidRootPart.Position - Player.Character.HumanoidRootPart.Position).magnitude < 4.35 and Model:FindFirstChild("JustGotHitWithStarPower") == nil and Model:FindFirstChildOfClass("Humanoid").Health > 0.001 then
								--rStorage.VampireDamage:FireServer(Model:FindFirstChildOfClass("Humanoid"), 1e-200, 0.25, Vector3.new(0, 0, -0))
								wait(0.015)
								rStorage.GravityForce:FireServer()
								rStorage.BurnDamage:FireServer(Model:FindFirstChildOfClass("Humanoid"), Model.Torso.CFrame, math.huge, 0.3, Model.Torso.CFrame:Inverse().lookVector * 0 * Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://5126807492", math.random(98,106)/100, 7.5)
								rStorage.HamonSword:FireServer(Model:FindFirstChildOfClass("Humanoid"),1,1,Vector3.new(math.huge,math.huge,math.huge))
								--rStorage.VampireRush:FireServer(Model:FindFirstChildOfClass("Humanoid"), CFrame.new(0, -30, 0), 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
								coroutine.resume(coroutine.create(function()
									for i = 1, 10 do
										rStorage.Damage12:FireServer(Model:FindFirstChildOfClass("Humanoid"), Model.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0, Vector3.new(0, 0, 0), 0.075, "rbxassetid://1693499499", 1, 0)
										rStorage.Damage3:FireServer(Model:FindFirstChildOfClass("Humanoid"), Model.Torso.CFrame * CFrame.new(0, -12, 0), 99, 0, Vector3.new(0, 0, 0), 0.075, "rbxassetid://1693499499", 1, 0)
										rStorage.HamonSword:FireServer(Model:FindFirstChildOfClass("Humanoid"),1,1,Vector3.new(9999999999999999999999999999999e9999999999999999999999,9999999999999999999999999999999999999999999999999999999999999999999999999999e99999999999999999999,99999999999999999999999999999999999999999999999e9999999999999999999999999))
                                        rStorage.Damage12Enderman:FireServer(Model:FindFirstChildOfClass("Humanoid"), Model.Torso.CFrame * CFrame.new(0, -12, 0), 99, 0, Vector3.new(0, 0, 0), 1e-320, "rbxassetid://1693499499", 1, 0)
									end
								end))
								local Value = Instance.new("StringValue")
								Value.Parent = Model
								Value.Name = "JustGotHitWithStarPower"
								Value.Value = "Gaming"
								game:GetService("Debris"):AddItem(Value, 0.2)
							end
						end
					end
				end
				wait()
				Time = Time + 0.05
				if Time > 1 then
					break
				end
			end
		end)
	end))
end

function HitBox(Part, Range, StayTime, HitTime, Damage, Knockback, KnockbackTime, CustomHitSound, NoRag, ExtraEffect, ExtraValue, InfiniteType, DisableType, BeatdownType)
	coroutine.resume(coroutine.create(function()
		pcall(function()
			local Humanoids = workspace.Entities:GetChildren()
			local Time = 0
			while true do
				Humanoids = workspace.Entities:GetChildren()
				for i = 1, #Humanoids do
					local Model = Humanoids[i]
					if Model ~= Player.Character then
						if Model:FindFirstChildOfClass("Humanoid") then
							if Model:FindFirstChild("HumanoidRootPart") then
								if (Model.HumanoidRootPart.Position - Part.Position).magnitude < Range and Model:FindFirstChild("JustGotHitten") == nil and Model:FindFirstChildOfClass("Humanoid").Health > 0.001 then
									if DisableType == true then
										God(Model:FindFirstChildOfClass("Humanoid"))
										rStorage.Knock:FireServer(Model:FindFirstChildOfClass("Humanoid"))
									end
									if BeatdownType == true and Beatdown == false then
										BeatdownHumanoid(Model:FindFirstChildOfClass("Humanoid"))
									end
									if Damage < 0 then
										if Model:FindFirstChildOfClass("Humanoid").Health < Model:FindFirstChildOfClass("Humanoid").MaxHealth then
											--rStorage.VampireRush:FireServer(Model:FindFirstChildOfClass("Humanoid"), CFrame.new(0, -30, 0), Damage, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
											rStorage.BurnDamage:FireServer(Model:FindFirstChildOfClass("Humanoid"), Model.Head.CFrame, Damage, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
										end
									else
										coroutine.resume(coroutine.create(function()
											if InfiniteType == true then
												for i = 1, 10 do
													rStorage.Damage12:FireServer(Model:FindFirstChildOfClass("Humanoid"), Model.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0.3, Vector3.new(-9999,-9999,-9999) * 9e999, 1e-320, "rbxassetid://1693499499", 1, 0)
												end
												--rStorage.VampireRush:FireServer(Model:FindFirstChildOfClass("Humanoid"), CFrame.new(0, -30, 0), 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
												rStorage.BurnDamage:FireServer(Model:FindFirstChildOfClass("Humanoid"), Model.Head.CFrame, 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
											end
										end))
										if Knockback == nil then
											rStorage.Damage3:FireServer(Model:FindFirstChildOfClass("Humanoid"), Part.CFrame, Damage, 0, Vector3.new(0,0,0), 0.045, CustomHitSound or "rbxassetid://1693499499", 1, 4.5)
										else
											rStorage.Damage:FireServer(Model:FindFirstChildOfClass("Humanoid"), Part.CFrame, 0, KnockbackTime, Knockback, "rbxassetid://241837157", 0, Color3.fromRGB(255, 255, 255), "rbxassetid://", 1, 0)
											rStorage.Damage3:FireServer(Model:FindFirstChildOfClass("Humanoid"), Part.CFrame, Damage, KnockbackTime, Knockback, 0.06, CustomHitSound or "rbxassetid://1693499499", 0.985, 4.5)
											if NoRag == nil or NoRag == false then
												rStorage.Knock:FireServer(Model:FindFirstChildOfClass("Humanoid"))
											end
										end
										if ExtraEffect == true then
											rStorage.Damage12:FireServer(Model:FindFirstChildOfClass("Humanoid"), Part.CFrame, 0.001, 0, Vector3.new(0,0,0), ExtraValue/15, "rbxassetid://1693499499", 1, 0)
										end
									end
									local Value = Instance.new("StringValue")
									Value.Parent = Model
									Value.Name = "JustGotHitten"
									Value.Value = "Gaming"
									game:GetService("Debris"):AddItem(Value, HitTime)
								end
							end
							if Model:FindFirstChild("Stand") then
								if Model.Stand:FindFirstChild("Stand Torso") then
									if (Model.Stand["Stand Torso"].Position - Part.Position).magnitude < Range and Model:FindFirstChild("JustGotHitten") == nil and Model:FindFirstChildOfClass("Humanoid").Health > 0.001 then
										if DisableType == true then
											God(Model:FindFirstChildOfClass("Humanoid"))
											rStorage.Knock:FireServer(Model:FindFirstChildOfClass("Humanoid"))
										end
										if Damage < 0 then
											if Model:FindFirstChildOfClass("Humanoid").Health < Model:FindFirstChildOfClass("Humanoid").MaxHealth then
												--rStorage.VampireRush:FireServer(Model:FindFirstChildOfClass("Humanoid"), CFrame.new(0, -30, 0), Damage, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
												rStorage.BurnDamage:FireServer(Model:FindFirstChildOfClass("Humanoid"), Model.Head.CFrame, Damage, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
											end
										else
											coroutine.resume(coroutine.create(function()
												if InfiniteType == true then
													for i = 1, 10 do
														rStorage.Damage12:FireServer(Model:FindFirstChildOfClass("Humanoid"), Model.Torso.CFrame * CFrame.new(0, -5, 0), 99, 0.3, Vector3.new(-9999,-9999,-9999) * 9e999, 1e-320, "rbxassetid://1693499499", 1, 0)
													end
													--rStorage.VampireRush:FireServer(Model:FindFirstChildOfClass("Humanoid"), CFrame.new(0, -30, 0), 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", -9e999, Color3.new(1, 1, 1), "rbxassetid://935843979", 0, 0)
													rStorage.BurnDamage:FireServer(Model:FindFirstChildOfClass("Humanoid"), Model.Head.CFrame, 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "rbxassetid://260430079", 1, 0)
												end
											end))
											if Knockback == nil then
												rStorage.Damage3:FireServer(Model:FindFirstChildOfClass("Humanoid"), Part.CFrame, Damage, 0, Vector3.new(0,0,0), 0.045, CustomHitSound or "rbxassetid://1693499499", 1, 4.5)
											else
												rStorage.Damage:FireServer(Model:FindFirstChildOfClass("Humanoid"), Part.CFrame, 0, KnockbackTime, Knockback, "rbxassetid://241837157", 0, Color3.fromRGB(255, 255, 255), "rbxassetid://", 1, 0)
												rStorage.Damage3:FireServer(Model:FindFirstChildOfClass("Humanoid"), Part.CFrame, Damage, KnockbackTime, Knockback, 0.06, CustomHitSound or "rbxassetid://1693499499", 0.985, 4.5)
												if NoRag == nil or NoRag == false then
													rStorage.Knock:FireServer(Model:FindFirstChildOfClass("Humanoid"))
												end
											end
											if ExtraEffect == true then
												rStorage.Damage12:FireServer(Model:FindFirstChildOfClass("Humanoid"), Part.CFrame, 0.001, 0, Vector3.new(0,0,0), ExtraValue/15, "rbxassetid://1693499499", 1, 0)
											end
										end
										local Value = Instance.new("StringValue")
										Value.Parent = Model
										Value.Name = "JustGotHitten"
										Value.Value = "Gaming"
										game:GetService("Debris"):AddItem(Value, HitTime)
									end
								end
							end
						end
					end
				end
				wait()
				Time = Time + 0.05
				if Time > StayTime then
					break
				end
			end
		end)
	end))
end

function GetRandomStarmanSong(Force)
	local Number
	local Chosen = nil
	if Force then
		Number = Force 
	else
		Number = math.random(1, 22)
	end
	if Number == 1 then
		Chosen = Songs.Remix
	elseif Number == 2 then
		Chosen = Songs.ColorSplash
	elseif Number == 3 then
		Chosen = Songs.NewerDS
	elseif Number == 4 then
		Chosen = Songs.Remix2
	elseif Number == 5 then
		Chosen = Songs.SM64
	elseif Number == 6 then
		Chosen = Songs.Tour
	elseif Number == 7 then
		Chosen = Songs.MK64
	elseif Number == 8 then
		Chosen = Songs.Sport
	elseif Number == 9 then
		Chosen = Songs.Sticker
	elseif Number == 10 then
		Chosen = Songs.DD
	elseif Number == 11 then
		Chosen = Songs.Run
	elseif Number == 12 then
		Chosen = Songs.SPM
	elseif Number == 13 then
		Chosen = Songs.Song
	elseif Number == 14 then
		Chosen = Songs.Bear
	elseif Number == 15 then
		Chosen = Songs.SMRemix
	elseif Number == 16 then
		Chosen = Songs.NSMBWII
	elseif Number == 17 then
		Chosen = Songs.Fazed
	elseif Number == 18 then
		Chosen = Songs.FazedDrum
	elseif Number == 19 then
		Chosen = Songs.SMK
	elseif Number == 20 then
		Chosen = Songs.MKWii
	elseif Number == 21 then
		Chosen = Songs.Dolar
	elseif Number == 22 then
		Chosen = Songs.Remix3
	elseif Number == 23 then
		Chosen = Songs.Frogs
	elseif Number == 24 then
		Chosen = Songs.NoNo
	elseif Number == 25 then
		Chosen = Songs.BLAZING
	elseif Number == 26 then
		Chosen = Songs.NIGHTOF
	elseif Number == 27 then
		Chosen = Songs.Running
	elseif Number == 28 then
		Chosen = Songs.Chunga
	elseif Number == 29 then
		Chosen = Songs.Hime
        elseif Number == 30 then
        Chosen = Songs.Discord
	end
	if Chosen ~= nil then
		return {Chosen.ID, Chosen.Volume, Chosen.Pitch}
	end
end

